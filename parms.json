{
    "version": "1.1",
    "general": {
        "nocolor": false,
        "prefix": [
            {
                "0": "${PWD}"
            },
            {
                "1": "0/source"
            },
            {
                "2": "0/object"
            },
            {
                "3": "0/macros"
            }
        ]
    },
    "asm": {
        "language": "merlin16plus", 
        "cpu": "M6502",
        "start_mx": 2,
        "listmode": "on",
        "casesen": true,
        "start_lst": false,
        "showmx": true,
        "allowduplicate": true,
        "trackrep": false,
        "merlinerrors": true,
        "m32vars": false,
        "allowA": true,
        "allowLocal": true,
        "allowColon": true,
        "repsep": "force", 
        "linebytes": 4,
        "line2bytes": 8
    },
    "linker": {},
    "format": {
        "tabs": "12,18,30,48"
    }
}